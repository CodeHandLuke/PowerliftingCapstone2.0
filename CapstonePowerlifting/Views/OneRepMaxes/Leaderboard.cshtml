@model IEnumerable<PowerliftingCapstone.Models.OneRepMax>

@{
    ViewBag.Title = "Leaderboard";
}

<h2>Leaderboard</h2>

<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.Date)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Squat)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Bench)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Deadlift)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Total)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Wilks)
		</th>
		@*<th>
			@Html.DisplayNameFor(model => model.UserId)
		</th>*@
		<th></th>
	</tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Squat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bench)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deadlift)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wilks)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>*@
    </tr>
}

</table>
